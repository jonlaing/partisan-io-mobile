{
  "name": "Partisan",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "test": "jest"
  },
  "dependencies": {
    "@exponent/react-native-navigator": "^0.4.1",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "jquery": "^2.2.0",
    "moment": "^2.11.1",
    "react": "^0.14.8",
    "react-dom": "~0.14.0",
    "react-native": "^0.23.1",
    "react-native-button": "^1.3.1",
    "react-native-gifted-messenger": "0.0.24",
    "react-native-keyboard-spacer": "^0.1.6",
    "react-native-markdown": "^0.1.1",
    "react-native-side-menu": "^0.18.0",
    "react-native-vector-icons": "^1.1.1"
  },
  "eslintConfig": {
    "plugins": [
      "react"
    ],
    "env": {
      "node": true,
      "es6": true
    },
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    },
    "rules": {
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ],
      "no-use-before-define": 0,
      "no-underscore-dangle": 0,
      "quotes": [
        0,
        "single"
      ]
    }
  },
  "devDependencies": {
    "babel-jest": "^9.0.2",
    "babel-preset-es2015": "*",
    "babel-preset-react": "*",
    "babel-preset-react-native": "^1.5.7",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "babel-preset-stage-1": "^6.5.0",
    "fetch-mock": "^4.4.0",
    "jest-cli": "^11.0.2",
    "react-addons-test-utils": "~0.14.0",
    "react-native-mock": "0.0.7"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/jestSupport/preprocessor.js",
    "setupEnvScriptFile": "<rootDir>/jestSupport/env.js",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "packager/react-packager/src/Activity/"
    ],
    "testFileExtensions": [
      "js"
    ],
    "unmockedModulePathPatterns": [
      "promise",
      "source-map"
    ]
  }
}
